{
  "name": "seattle-humane-dbs-notes-chrome-extension",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "chrome-webstore-upload-cli": "^1.0.0",
    "dot-json": "^1.0.3"
  },
  "scripts": {
    "deploy-new-version": "npm run update-version && npm run release-cws",
    "release": "cd browser && webstore upload --auto-publish",
    "update-version": "dot-json browser/manifest.json version $EXTENSION_VERSION"
  }
}
